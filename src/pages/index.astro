---
import Layout from "../layouts/Layout.astro";
import KortsamligDailymix from "../components/KortsamligDailymix.astro";
import Kort from "../components/Kort.astro";
import "../styles/style.css";
---

<Layout titel="Spotify">
  <KortsamligDailymix titel="De fedeste mix til Peter Ulf">
    <Kort
      farve="var(--myblue)"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=1&q=95&w=360&h=360&fit=fill"
    />
    <Kort
      farve="red"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=2&q=95&w=360&h=360&fit=fill"
    />
    <Kort
      farve="green"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=3&q=95&w=360&h=360&fit=fill"
    />
    <Kort
      farve="purple"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=4&q=95&w=360&h=360&fit=fill"
    />
  </KortsamligDailymix>
  <KortsamligDailymix titel="De fedeste mix til Troels">
    <Kort
      farve="blue"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=1&q=95&w=360&h=360&fit=fill"
    />
    <Kort
      farve="blue"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=2&q=95&w=360&h=360&fit=fill"
    />
    <Kort
      farve="blue"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=3&q=95&w=360&h=360&fit=fill"
    />
    <Kort
      farve="blue"
      mix="02"
      kunstnerliste="Hund i snor, Lady Gaga"
      imgsrc="https://via.assets.so/album.png?id=4&q=95&w=360&h=360&fit=fill"
    />
  </KortsamligDailymix>
</Layout>
